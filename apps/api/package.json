    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.7",
    "@nestjs/config": "^3.1.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
